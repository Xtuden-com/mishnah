         <ref name="mishnahw_att.global.attribute.xmllang"/>
         <ref name="mishnahw_att.global.attribute.xmlbase"/>
         <ref name="mishnahw_att.global.attribute.xmlspace"/>
         <ref name="mishnahw_att.global.rendition.attribute.rend"/>
         <ref name="mishnahw_att.global.rendition.attribute.style"/>
         <ref name="mishnahw_att.global.rendition.attribute.rendition"/>
         <ref name="mishnahw_att.global.linking.attribute.corresp"/>
         <ref name="mishnahw_att.global.linking.attribute.synch"/>
         <ref name="mishnahw_att.global.linking.attribute.sameAs"/>
         <ref name="mishnahw_att.global.linking.attribute.copyOf"/>
         <ref name="mishnahw_att.global.linking.attribute.next"/>
         <ref name="mishnahw_att.global.linking.attribute.prev"/>
         <ref name="mishnahw_att.global.linking.attribute.exclude"/>
         <ref name="mishnahw_att.global.linking.attribute.select"/>
         <ref name="mishnahw_att.global.facs.attribute.facs"/>
         <ref name="mishnahw_att.global.change.attribute.change"/>
