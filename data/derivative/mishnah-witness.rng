            </optional>
            <optional>
               <ref name="mishnahw_handDesc"/>
            </optional>
      
         </group>
         <ref name="mishnahw_att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mishnahw_objectDesc">
      <element name="objectDesc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a description of the physical components making up the object which is being described. [10.7.1. Object Description]</a:documentation>
         <choice>
      
            <oneOrMore>
               <ref name="mishnahw_model.pLike"/>
            </oneOrMore>
      
            <group>
        
               <optional>
                  <ref name="mishnahw_supportDesc"/>
               </optional>
        
        
               <optional>
                  <ref name="mishnahw_layoutDesc"/>
               </optional>
        
