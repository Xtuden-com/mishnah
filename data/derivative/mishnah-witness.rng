         <ref name="mishnahw_att.damaged.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mishnahw_damageSpan">
      <element name="damageSpan">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(damaged span of text) marks the beginning of a longer sequence of text which is damaged in some way but still legible. [11.3.3.1. Damage, Illegibility, and Supplied Text]</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="digital_mishnah_witness-damageSpan-spanTo-constraint-10">
            <rule context="tei:damageSpan">
               <assert xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       test="@spanTo">
The @spanTo attribute of <name/> is required.</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="digital_mishnah_witness-damageSpan-spanTo_fr-constraint-11">
            <rule context="tei:damageSpan">
               <assert xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       test="@spanTo">L'attribut spanTo est requis.</assert>
            </rule>
         </pattern>
         <ref name="mishnahw_att.global.attribute.xmlid"/>
         <ref name="mishnahw_att.global.attribute.n"/>
         <ref name="mishnahw_att.global.attribute.xmllang"/>
         <ref name="mishnahw_att.global.attribute.xmlbase"/>
         <ref name="mishnahw_att.global.attribute.xmlspace"/>
         <ref name="mishnahw_att.global.rendition.attribute.rend"/>
         <ref name="mishnahw_att.global.rendition.attribute.style"/>
         <ref name="mishnahw_att.global.rendition.attribute.rendition"/>
         <ref name="mishnahw_att.global.linking.attribute.synch"/>
         <ref name="mishnahw_att.global.linking.attribute.sameAs"/>
         <ref name="mishnahw_att.global.linking.attribute.copyOf"/>
         <ref name="mishnahw_att.global.linking.attribute.next"/>
         <ref name="mishnahw_att.global.linking.attribute.prev"/>
         <ref name="mishnahw_att.global.linking.attribute.exclude"/>
         <ref name="mishnahw_att.global.linking.attribute.select"/>
         <ref name="mishnahw_att.global.facs.attribute.facs"/>
         <ref name="mishnahw_att.global.change.attribute.change"/>
         <ref name="mishnahw_att.global.responsibility.attribute.cert"/>
         <ref name="mishnahw_att.global.responsibility.attribute.resp"/>
         <ref name="mishnahw_att.damaged.attributes"/>
         <ref name="mishnahw_att.typed.attributes"/>
         <attribute name="corresp">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(corresponds) points to elements that correspond to the current element in some way.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI"/>
               </oneOrMore>
            </list>
         </attribute>
         <attribute name="spanTo">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the end of a span initiated by the element bearing this attribute.</a:documentation>
            <data type="anyURI"/>
         </attribute> 
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="digital_mishnah_witness-damageSpan-spanTo-spanTo-2-constraint-10">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:*[@spanTo]">
               <sch:assert test="id(substring(@spanTo,2)) and following::*[@xml:id=substring(current()/@spanTo,2)]">
The element indicated by @spanTo (<sch:value-of select="@spanTo"/>) must follow the current element <sch:name/>
                  </sch:assert>
            </sch:rule>
         </pattern>
         <empty/>
      </element>
   </define>
   <define name="mishnahw_fw">
      <element name="fw">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(forme work) contains a running head (e.g. a header, footer), catchword, or similar material appearing on the current page. [11.6. Headers, Footers, and Similar Matter]</a:documentation>
