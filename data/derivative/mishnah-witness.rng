         </zeroOrMore>
         <ref name="mishnahw_att.global.attributes"/>
         <ref name="mishnahw_att.timed.attributes"/>
         <ref name="mishnahw_att.editLike.attribute.evidence"/>
         <ref name="mishnahw_att.editLike.attribute.instant"/>
         <ref name="mishnahw_att.dimensions.attribute.unit"/>
         <ref name="mishnahw_att.dimensions.attribute.quantity"/>
         <ref name="mishnahw_att.dimensions.attribute.precision"/>
         <ref name="mishnahw_att.dimensions.attribute.scope"/>
